package br.com.projeto.rh.model;



public class CargosDTO {

	
	private String nome;

	
	public CargosDTO(String nome) {
		super();
		this.nome = nome;
	}


	public CargosDTO() {
		super();
		// TODO Auto-generated constructor stub
	}


	public String getNome() {
		return nome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	}
	
	
	
	
}
